# Create a dataframe with the given age values
ages <- c(13, 15, 16, 16, 19, 20, 20, 21, 22, 22, 25, 25, 25, 25, 30, 33, 33, 35, 35, 35, 35, 36, 40, 45, 46, 52, 70)
age_df <- data.frame(Age = ages)

# Calculate Q1 and Q3
q1 <- quantile(age_df$Age, 0.25)
q3 <- quantile(age_df$Age, 0.75)

# Print Q1 and Q3
cat("First Quartile (Q1):", q1, "\n")
cat("Third Quartile (Q3):", q3, "\n")
