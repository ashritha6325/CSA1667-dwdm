# Create the dataframe
data <- data.frame(
  Person = c("Gopu", "Babu", "Baby", "Gopal", "Krishna", "Jai", "Dev", "Malini", "Hema", "Anu"),
  Vegetarian = c("yes", "yes", "yes", "no", "yes", "no", "no", "yes", "yes", "yes")
)

# Count the number of vegetarians and non-vegetarians
veg_count <- sum(data$Vegetarian == "yes")
non_veg_count <- sum(data$Vegetarian == "no")

# Print the counts
cat("Number of vegetarians:", veg_count, "\n")
cat("Number of non-vegetarians:", non_veg_count, "\n")

# Determine which type has the greater count
if (veg_count > non_veg_count) {
  cat("There are more vegetarians.\n")
} else if (non_veg_count > veg_count) {
  cat("There are more non-vegetarians.\n")
} else {
  cat("The number of vegetarians and non-vegetarians is equal.\n")
}
