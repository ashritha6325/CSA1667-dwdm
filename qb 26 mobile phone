# Create the dataframe
data <- data.frame(
  x = c(4, 1, 5, 7, 10, 2, 50, 25, 90, 36),
  y = c(12, 5, 13, 19, 31, 7, 153, 72, 275, 110)
)

# Create the scatter plot using base R
plot(data$x, data$y,
     main = "Scatter Plot of Mobile Phones Sold vs Money",
     xlab = "Number of Mobile Phones Sold",
     ylab = "Money",
     pch = 19, # point type
     col = "blue")
